{
  "name": "port-ferry-ts",
  "version": "1.0.0",
  "description": "port-ferry is a utility that ferries your HTTP requests between different local ports (eg for use in microservice app environments), with JSON-configurable port mappings and status code/error logging. Not intended for production use.",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-erin-collective/port-ferry-ts"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "npx ts-node src/index.ts"
  },
  "keywords": [
    "proxy",
    "port-forwarding",
    "http-proxy",
    "route-mapping",
    "debugging",
    "utility"
  ],
  "author": "the-erin-collective",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/express-http-proxy": "^1.6.4",
    "@types/node": "^20.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0"
  }
}
